#
# ~/.profile
#

# export JAVA_HOME=/usr/lib/jvm/java-8-openjdk

export TERMINAL="darkblue xfce4-terminal"
export EDITOR="vim"
export VISUAL=$EDITOR
export PATH=$PATH:$HOME/bin:$HOME/.screenlayout/:$HOME/.dotnet/tools:$HOME/go/bin
export XDG_CONFIG_HOME="$HOME/.config"

export DASHT_DOCSETS_DIR=$HOME/.local/share/Zeal/Zeal/docsets

export GTK2_RC_FILES="$HOME/.gtkrc-2.0"
export QT_QPA_PLATFORMTHEME='qt5ct'
export TCLLIBPATH=$HOME/.local/share/tk-themes
export PYTHONRC="$HOME/.pythonrc"
export npm_config_prefix="$HOME/.local"
export DOTNET_CLI_TELEMETRY_OPTOUT=1
export DOTNET_ROOT=/opt/dotnet
export MSBuildSDKsPath="/opt/dotnet/sdk/2.2.108/Sdks/";
export MOZ_WEBRENDER=1
export MOZ_USE_XINPUT2=1
export SYNCTHING_ADDRESS=localhost:8080

# Customization

export ROFIICONSENABLED=true



# Added by Toolbox App
export PATH="$PATH:/home/diogo/.local/share/JetBrains/Toolbox/scripts"

